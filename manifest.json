{
  "manifest_version": 3,
  "name": "chorus",
  "version": "1.0",
  "description": "Compare LLMs in their native browser UI.",
  "icons": {
    "16": "icons/chorus16.png",
    "48": "icons/chorus48.png",
    "128": "icons/chorus128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "omnibox": {
    "keyword": "ch"
  },
  "permissions": [
    "tabs"
  ]
} 